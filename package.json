{
    "name": "snapp",
    "version": "3.1.0",
    "description": "Snapp! transforms you Snap! projects into standalone executables.",
    "author": {
        "name": "Adrian Hintze",
        "email": "hintze.adrian@gmail.com",
        "url": "https://github.com/Rydion/"
    },
    "private": true,
    "scripts": {
        "run": "node build/snapp/Snapp.js",
        "full-setup": "npm run copy-resources && npm run download-nw && npm run build",
        "prebuild": "rimraf build/logs && rimraf build/upload",
        "build": "npm run build-server && npm run build-client",
        "prebuild-server": "rimraf build/snapp",
        "build-server": "tsc -p server-tsconfig.json",
        "prebuild-client": "rimraf build/WebContent",
        "build-client": "webpack --progress --profile",
        "copy-resources": "npm run copy-snap && npm run copy-icons && npm run copy-conf && npm run create-conf-file",
        "copy-snap": "npm run copy-snap-full && npm run copy-snap-reduced",
        "precopy-snap-full": "rimraf build/resources/snap/full",
        "copy-snap-full": "copyfiles -u 2 \"node_modules/snap-4-snapp-full/**/*\" \"build/resources/snap/full/files\" && copyfiles -u 2 \"node_modules/snap-4-snapp-full/gui.js\" \"build/resources/snap/full/gui\"",
        "postcopy-snap-full": "rimraf build/resources/snap/full/files/gui.js && rimraf build/resources/snap/full/files/package.json",
        "precopy-snap-reduced": "rimraf build/resources/snap/reduced",
        "copy-snap-reduced": "rimraf build/resources/snap/reduced && copyfiles -u 2 \"node_modules/snap-4-snapp-reduced/**/*\" \"build/resources/snap/reduced/files\" && copyfiles -u 2 \"node_modules/snap-4-snapp-reduced/gui.js\" \"build/resources/snap/reduced/gui\"",
        "postcopy-snap-reduced": "rimraf build/resources/snap/reduced/files/gui.js && rimraf build/resources/snap/reduced/files/package.json",
        "precopy-icons": "rimraf build/resources/icons",
        "copy-icons": "copyfiles -u 1 \"icons/**/*\" \"build/resources/icons\"",
        "precopy-conf": "rimraf build/resources/conf",
        "copy-conf": "copyfiles -u 1 \"binary_conf/**/*\" \"build/resources/conf\"",
        "precreate-conf-file": "rimraf build/snapp_conf.json",
        "create-conf-file": "echo { \"port\": 80, \"uploadFileSizeLimit\": 100000000, \"compressStaticFiles\": true } >> build/snapp_conf.json",
        "download-nw": "npm run download-nw-win",
        "download-nw-win": "npm run download-nw-win32 && npm run download-nw-win64",
        "predownload-nw-win32": "rimraf build/resources/nw/win32",
        "download-nw-win32": "download --extract --strip 1 --out build/resources/nw/win32/lib/ https://dl.nwjs.io/v0.32.4/nwjs-v0.32.4-win-ia32.zip",
        "postdownload-nw-win32": "rimraf build/resources/nw/win32/lib/nw.exe && copyfiles -u 3 nw/win32/exe/nw.exe build/resources/nw/win32/exe/",
        "predownload-nw-win64": "rimraf build/resources/nw/win64",
        "download-nw-win64": "download --extract --strip 1 --out build/resources/nw/win64/lib/ https://dl.nwjs.io/v0.32.4/nwjs-v0.32.4-win-x64.zip",
        "postdownload-nw-win64": "rimraf build/resources/nw/win64/lib/nw.exe && copyfiles -u 3 nw/win64/exe/nw.exe build/resources/nw/win64/exe/",
        "predownload-nw-mac64": "rimraf build/resources/nw/mac64",
        "download-nw-mac64": "download --extract --strip 2 --out build/resources/nw/mac64/ https://dl.nwjs.io/v0.32.4/nwjs-v0.32.4-osx-x64.zip"
    },
    "engines": {
        "node": ">=10.4.1",
        "npm": ">=6.1.0"
    },
    "dependencies": {
        "archiver": "2.1.1",
        "body-parser": "1.18.3",
        "compression": "1.7.3",
        "download-cli": "1.1.1",
        "express": "4.16.3",
        "mkdirp": "0.5.1",
        "mime": "2.3.1",
        "move-cli": "1.2.0",
        "multer": "1.3.1",
        "sax": "1.2.4",
        "stream-to-array": "2.3.0",
        "winston": "3.0.0",
        "winston-daily-rotate-file": "3.3.2"
    },
    "devDependencies": {
        "@angular/animations": "6.1.4",
        "@angular/common": "6.1.4",
        "@angular/compiler": "6.1.4",
        "@angular/core": "6.1.4",
        "@angular/forms": "6.1.4",
        "@angular/http": "6.1.4",
        "@angular/platform-browser": "6.1.4",
        "@angular/platform-browser-dynamic": "6.1.4",
        "@angular/router": "6.1.4",
        "@types/archiver": "2.1.2",
        "@types/body-parser": "1.17.0",
        "@types/compression": "0.0.36",
        "@types/express": "4.16.0",
        "@types/file-saver": "1.3.0",
        "@types/mime": "2.0.0",
        "@types/mkdirp": "0.5.2",
        "@types/multer": "1.3.7",
        "@types/node": "10.9.3",
        "@types/sax": "1.0.1",
        "@types/stream-to-array": "2.3.0",
        "@types/winston": "2.4.4",
        "@types/xmldoc": "1.1.3",
        "angular2-busy": "2.0.4",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "5.2.0",
        "bootstrap": "4.1.3",
        "concurrently": "4.0.1",
        "copyfiles": "2.0.0",
        "core-js": "2.5.7",
        "css-loader": "1.0.0",
        "file-loader": "2.0.0",
        "file-saver": "1.3.8",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "mini-css-extract-plugin": "0.4.2",
        "ngx-modialog": "6.0.0",
        "null-loader": "0.1.1",
        "raw-loader": "0.5.1",
        "rimraf": "2.6.2",
        "rxjs": "6.2.2",
        "rxjs-compat": "6.2.2",
        "snap-4-snapp-full": "git+https://git@github.com/Rydion/Snap-4-Snapp.git#full-v3.0.0",
        "snap-4-snapp-reduced": "git+https://git@github.com/Rydion/Snap-4-Snapp.git#reduced-v3.0.0",
        "style-loader": "0.23.0",
        "to-string-loader": "1.1.5",
        "typescript": "3.0.1",
        "webpack": "4.17.1",
        "webpack-cli": "3.1.0",
        "webpack-log": "2.0.0",
        "webpack-merge": "4.1.4",
        "zone.js": "0.8.26"
    }
}
